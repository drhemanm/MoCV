# =============================================================================
# MoCV.mu Development Environment
# =============================================================================

# Application Settings
VITE_APP_NAME="MoCV.mu"
VITE_APP_VERSION="1.0.0"
VITE_APP_ENV="development"
VITE_DEBUG_MODE="true"
VITE_DEBUG_LOGS="true"

# API Configuration
VITE_API_BASE_URL="http://localhost:3000"

# OpenAI Configuration (Optional - leave empty if you don't have a key yet)
VITE_OPENAI_API_KEY= VITE_OPENAI_API_KEY
VITE_OPENAI_MODEL="gpt-4"
VITE_OPENAI_MAX_TOKENS="2000"

# Feature Flags
VITE_FEATURE_AI="false"
VITE_FEATURE_ANALYTICS="false"
VITE_FEATURE_OFFLINE="true"

# Security Settings
VITE_RATE_LIMITING="true"
VITE_RATE_LIMIT_REQUESTS="100"
VITE_RATE_LIMIT_WINDOW="900000"


